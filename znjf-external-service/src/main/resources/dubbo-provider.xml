<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       default-lazy-init="false">

    <dubbo:consumer check="false"/>

    <!-- 服务消费者接口 -->
    <dubbo:reference interface="com.znjf33.useraccount.api.service.UserSignatureService" id="userSignatureService" retries="0"  timeout="30000"/>
    <dubbo:reference interface="com.znjf33.investment.api.service.BorrowUpdateService" id="borrowUpdateService" retries="0"/>
    <dubbo:reference interface="com.znjf33.general.api.service.NoticeTagService" id="noticeTagServiceImpl" retries="0" owner="mwj"/>
    <dubbo:reference interface="com.znjf33.general.api.service.SystemConfigService" id="systemConfigService" retries="0"  owner="mwj"/>

    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service interface="com.znjf33.external.api.service.SupplyChainLmjService" ref="supplyChainLmjServiceImpl" retries="0" timeout="80000" owner="mwj"/>
</beans>
